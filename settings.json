{
    "files.exclude": {
        "**/.eslintcache": true,
        "**/.next": true,
        "**/out": true,
        "**/yarn-error.log": true,
        "web-app/generated_assets": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/*.olean": true
    },
    "files.watcherExclude": {
        "**/.next": true,
        "**/.yarn-offline-mirror": true,
        "**/yarn.lock": true,
        "static-sites/**/.cache": true,
        "static-sites/artifacts": true,
        "static-sites/generated_assets": true,
        "web-app/build": true,
        "web-app/generated_assets": true
    },
    "search.exclude": {
        "**/.yarn-offline-mirror": true,
        "**/dist": true,
        "**/yarn.lock": true,
        "design/lib": true,
        "shared-modules/lib": true,
        "static-sites/**/.cache": true,
        "static-sites/artifacts": true,
        "static-sites/generated_assets": true,
        "web-app/build": true,
        "web-app/generated_assets": true
    },
    "eslint.enable": true,
    "eslint.workingDirectories": [
        {
            "directory": "./design",
            "changeProcessCWD": true
        },
        {
            "directory": "./web-app",
            "changeProcessCWD": true
        },
        {
            "directory": "./static-sites",
            "changeProcessCWD": true
        },
        {
            "directory": "./shared-modules",
            "changeProcessCWD": true
        },
        {
            "directory": "./reldash",
            "changeProcessCWD": true
        }
    ],
    "editor.codeActionsOnSave": {
        // Use eslint-vscode for auto-fixing files. This includes prettier.
        "source.fixAll.eslint": true
    },
    // Enable formatOnSave by default
    "editor.formatOnSave": true,
    // Disable the formatOnSave for languages that should instead
    // use the "eslint --fix" equivalent via codeActionsOnSave
    "[javascript]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.formatOnSave": false
    },
    "[typescriptreact]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // Set Yarn as the package manager
    "eslint.packageManager": "yarn",
    "npm.packageManager": "yarn",
    "typescript.tsdk": "node_modules/typescript/lib",
    // Tyler styles begin here
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "breadcrumbs.enabled": false,
    "debug.console.fontSize": 14,
    "debug.showBreakpointsInOverviewRuler": true,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontSize": 14,
    "editor.rulers": [
        100
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 100,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 300,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autoStash": true,
    "gitblame.statusBarMessageFormat": "Last edited by: ${author.name} ( ${time.ago} )",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "problems.decorations.enabled": false,
    "python.formatting.blackArgs": [
        "--line-length=100"
    ],
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "python.linting.flake8Args": [
        "--ignore=E203,E501,F401,W503",
        "--max-line-length=100"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.mypyArgs": [
        "--follow-imports=silent",
        "--show-column-numbers"
    ],
    "python.linting.mypyEnabled": true,
    "python.linting.pylintArgs": [
        "--disable=unsubscriptable-object",
        "--errors-only",
        "--generated-members=numpy.* ,torch.* ,cv2.* , cv.*"
    ],
    "python.linting.pylintEnabled": true,
    "python.sortImports.args": [
        "--line-length=100",
        "--profile=black"
    ],
    "search.smartCase": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.shell.osx": "/usr/local/bin/fish",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.tabCloseButton": "left",
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 24,
    // "[python]": {
    //     "editor.codeActionsOnSave": {
    //         "source.organizeImports": true
    //     }
    // },
}
